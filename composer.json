{
    "name": "propa/laravel-browscap",
    "description": "Browscap-PHP integration for Laravel 5-9",
    "keywords": ["laravel", "browscap", "browscap-php", "user-agent", "browser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Kulbakin",
            "email": "p.kulbakin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0 || ^8.0",
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "browscap/browscap-php": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "roave/doctrine-simplecache": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "Propa\\BrowscapPHP\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Propa\\BrowscapPHP\\BrowscapServiceProvider"
            ],
            "aliases": {
                "Browscap": "Propa\\BrowscapPHP\\Facades\\Browscap"
            }
        }
    }
}
